#include <stdio.h>
/* 
(getchar) enter tuşuna gereksinim duyar alınan karakter ekranda görünür.
(getch) enter tuşuna gereksinim duymaz alınan karakter ekranda görünmez.
(getche) enter tuşuna gereksinim duymaz alınan karakter ekranda görünür. 
*/
#include <conio.h>
 int main() 
 {  
   
   
  printf("deneme\n");
  
  printf("getchar\n");
  getchar();
  
  printf("getch\n");
  getch();
  
  printf("getche\n");
  getche();
  return 0; }
